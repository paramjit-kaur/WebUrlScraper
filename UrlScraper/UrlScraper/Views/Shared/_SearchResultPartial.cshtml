@model UrlScraper.Web.ViewModel.ResultDataViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="form-horizontal">
        <br />
        <br />
        <div class="form-group">
            <h4>  Total occurrences of "@Model.WebsiteUrl" in first @Model.RecordsToSearch records of Google search page as on @Model.CurrentDateTime.</h4>
        </div>
        <div class="form-group alert alert-success">
            Total results found :<span class="badge">@Model.TotalResults</span><br />
            Occurrences : @Model.Occurrences
        </div>
    </div>
</div>
